<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <ItemGroup>

    <NativeFramework Include="CoreText"/>
    <NativeFramework Include="Metal"/>
    <NativeFramework Include="OpenGLES"/>

    <NativeReference Include="$(MSBuildThisFileDirectory)..\..\runtimes\ios\native\libNoesis.a">
      <Kind>Static</Kind>
      <ForceLoad>True</ForceLoad>
      <SmartLink>False</SmartLink>
      <IsCxx>True</IsCxx>
    </NativeReference>

    <None Include="$(MSBuildThisFileDirectory)..\..\runtimes\ios\native\Symbols.cpp">
      <Link>Symbols.cpp</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>

  </ItemGroup>

  <PropertyGroup>

    <MtouchExtraArgs>
      $(MtouchExtraArgs) --dynamic-symbol-mode=code -gcc_flags "$(MSBuildThisFileDirectory)..\..\runtimes\ios\native\Symbols.cpp"
    </MtouchExtraArgs>

  </PropertyGroup>

</Project>
